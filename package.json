{
  "name": "royal-rangers-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Royal-Rangers Frontend",
  "scripts": {
    "preinstall": "npm install gulpjs/gulp-cli protractor ntl -g",
    "postinstall": "bower install",
    "prestart": "npm install",
    "vagrant": "cd scripts/vagrant/x64/ && vagrant up",
    "vagrant-stop": "cd scripts/vagrant/x64/ && vagrant halt -f",
    "vagrant-reload": "cd scripts/vagrant/x64/ && vagrant reload",
    "vagrant-destroy": "cd scripts/vagrant/x64/ && vagrant destroy -f",
    "vagrant-recreate": "cd scripts/vagrant/x64/ && vagrant destroy -f && vagrant up",
    "vagrant_x32-start": "cd scripts/vagrant/x32/ && vagrant up",
    "vagrant_x32-stop": "cd scripts/vagrant/x32/ && vagrant halt -f",
    "vagrant_x32-reload": "cd scripts/vagrant/x32/ && vagrant reload",
    "backend-start": "cd ../rr-api/ && gradlew.bat bootRun",
    "karma": "karma start tests/karma/karma.conf.js",
    "tests": "run-s karma",
    "selenium-server": "webdriver-manager update && webdriver-manager start",
    "selenium": "protractor tests/selenium/selenium.conf.js"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "bower": "1.8.0",
    "browser-sync": "2.18.12",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "4.0.0",
    "gulp-babel": "6.1.2",
    "gulp-clean": "0.3.2",
    "gulp-concat": "2.6.1",
    "gulp-copy": "1.0.0",
    "gulp-cssnano": "2.1.2",
    "gulp-if": "2.0.2",
    "gulp-ng-annotate": "2.0.0",
    "gulp-ngmin": "0.3.0",
    "gulp-put": "0.0.3",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.0",
    "http-proxy-middleware": "0.17.4",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-spec-reporter": "0.0.31",
    "protractor": "5.1.2",
    "npm-run-all": "4.0.2",
    "yargs": "8.0.2"
  }
}
